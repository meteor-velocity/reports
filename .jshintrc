{
  "node": true,
  "browser": false,
  "esnext": true,
  "bitwise": true,
  "camelcase": false,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": false,
  "globalstrict": false,
  "trailing": true,
  "smarttabs": true,
  "globals": {
    "process": false,
    "Meteor": false,
    "Deps": false,
    "DDP": false,
    "check": false,
    "Match": false,
    "Npm": false,
    "Random": false,
    "Package": false,
    "Assets": false,
    "Mongo": false,
    "MongoInternals": false,
    "DEBUG": false,
    "Velocity": false,
    "VelocityInternals": false,
    "VelocityTestReports": false,
    "VelocityAggregateReports": false,
    "$": false,
    "Jasmine": false,
    "jasmine": false,
    "spyOn": false,
    "it": false,
    "describe": false,
    "expect": false,
    "after": false,
    "before": false,
    "beforeEach": false,
    "afterEach": false,
    "xit": false,
    "xdescribe": false,
    "fit": false,
    "fdescribe": false
  },
  // Deactivate https://jslinterrors.com/expected-an-assignment-or-function-call
  "-W030": true
}
